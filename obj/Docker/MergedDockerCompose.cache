name: dockercompose11540984655745508182
services:
  modironline.log.api:
    build:
      context: E:\ModirOnline\ModirOnline
      dockerfile: Src/ModirOnline.Log/ModirOnline.Log.Api/Dockerfile
    container_name: modironline.logapi
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DatabaseSettings:ConnectionString: mongodb://modironline.log.db:27017
    image: modironlinelogapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8000"
        protocol: tcp
  modironline.log.db:
    container_name: modironline.logdb
    image: mongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: mongo_data
        target: /data/db
        volume: {}
networks:
  default:
    name: dockercompose11540984655745508182_default
volumes:
  mongo_data:
    name: dockercompose11540984655745508182_mongo_data